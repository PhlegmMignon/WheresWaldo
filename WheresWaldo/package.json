{
  "name": "whereswaldo",
  "version": "1.0.0",
  "description": "## Usage",
  "type": "module",
  "scripts": {
    "server": "npm run serverStart --prefix server",
    "client": "npm run dev --prefix client",
    "concurrent": "concurrently \"npm run server\"  \"npm run client\"",
    "dev": "npm run --prefix client vite",
    "build": "npm --prefix client run build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "start": "node ./bin/www",
    "devStart": "nodemon ./bin/www",
    "serverStart": "DEBUG=WheresWaldo:* npm run devStart",
    "build:css": "postcss ./public/stylesheets/tailwind.css -o ./public/stylesheets/output.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PhlegmMignon/WheresWaldo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PhlegmMignon/WheresWaldo/issues"
  },
  "homepage": "https://github.com/PhlegmMignon/WheresWaldo#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "luxon": "^3.4.3",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.4",
    "morgan": "~1.9.1",
    "node": "^20.7.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "mongodb-memory-server": "^8.16.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
